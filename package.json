{
  "name": "clarify",
  "version": "0.1.0",
  "private": true,
  "main": "electron-dist/electron/main.js",
  "scripts": {
    "build": "next build",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "dev": "next dev",
    "electron:compile": "tsc -p electron/tsconfig.json",
    "electron:dev": "concurrently -k \"cross-env NODE_ENV=development pnpm dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "electron:build": "cross-env BUILD_TARGET=electron pnpm build && pnpm electron:compile",
    "electron:dist": "pnpm electron:build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "format": "prettier --write .",
    "lint": "eslint --fix --cache",
    "next-typesafe-url": "next-typesafe-url",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.29",
    "@base-ui/react": "^1.1.0",
    "@lukemorales/query-key-factory": "^1.3.4",
    "@tanstack/react-form": "^1.27.7",
    "@tanstack/react-query": "^5.90.18",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.18",
    "better-sqlite3": "^12.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "electron-serve": "^2.1.1",
    "electron-store": "^10.0.1",
    "eslint-plugin-react-snob": "^0.0.26",
    "lucide-react": "^0.562.0",
    "next": "16.1.6",
    "next-typesafe-url": "^6.1.0",
    "nuqs": "^2.8.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-error-boundary": "^6.1.0",
    "shiki": "^3.21.0",
    "streamdown": "^2.0.1",
    "tailwind-merge": "^3.4.0",
    "use-stick-to-bottom": "^1.1.1",
    "yaml": "^2.8.2",
    "zod": "^4.3.5",
    "zod-validation-error": "^5.0.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "electron": "^35.1.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-better-tailwindcss": "^4.0.1",
    "eslint-plugin-perfectionist": "^5.3.1",
    "eslint-plugin-react-snob": "^0.0.26",
    "prettier": "^3.8.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "typescript-eslint": "^8.53.0",
    "wait-on": "^8.0.3"
  }
}
